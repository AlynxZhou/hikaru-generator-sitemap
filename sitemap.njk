<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for post in posts %}
  <url>
    <loc>{{ getURL(post.docPath) }}</loc>
    {% if post.updatedDate %}
    <lastmod>{{ post.updatedDate.toISOString() }}</lastmod>
    {% endif %}
  </url>
  {% endfor %}
</urlset>
